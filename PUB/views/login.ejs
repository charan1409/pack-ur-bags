<body>

  <section class="top">
    <header>
      <a href="#log" class="logo"><span>P</span>ACK <span>U</span>R <span>B</span>AGS</a>
    </header>
    <div id="log" class="form-container">
      <p id="temp"></p>
      <div class="top-btn">
        <!-- <div id="stat-btn"></div> -->
        <button type="button" class="toggle-btn">Sign in</button>
        <!-- <button type="button" class="toggle-btn" onclick="register()">Sign up</button> -->
      </div>
      <!-- Error Alert -->
      <% if(locals.errors !=null) {%>
        <% errors.forEach(function(error) { %>
          <div class="alert alert-danger log-alert" id="msg">
            <strong>Error! </strong>
            <%=error.msg%>
              <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
          </div>
          <% }); %>
            <% } %>
            <% if(locals.sucerrors !=null) {%>
              <% sucerrors.forEach(function(error) { %>
                <div class="alert bg-success log-suc" id="msg">
                  <strong>success! </strong>
                  <%=error.sucmsg%>
                    <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
                </div>
                <% }); %>
                  <% } %>
      <!-- Error Alert Ends-->

      <!-- Form Starts -->
                  
