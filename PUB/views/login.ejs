                <input type="submit" value="login now" class="login-btn"> <br> <br> <br>
                <div>
                  <input type="checkbox" id="remember">
                  <label for="remember">remember me</label> <br> <br>
                  <!-- <p>forget password? <a href="#">Click here</a></p> <br> -->
                  <a href="register"><button type="button" class="login-btn"
                      style="background-color: #FCD900;color: #060000;">Sign Up</button></a>
                  <p>Customer service: 9398717554</p>
                </div>
              </form>
    </div>

